<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
  </configSections>
  <log4net>
    <logger name="RD.EposServiceConsumer.SampleApplication">
      <level value="INFO"/>
      <appender-ref ref="RollingFileAppender"/>
    </logger>
    <appender name="RollingFileAppender" type="log4net.Appender.RollingFileAppender">
      <file value="RD.EposServiceConsumer.SampleApplication.log"/>
      <appendToFile value="true"/>
      <rollingStyle value="Size"/>
      <maxSizeRollBackups value="20"/>
      <maximumFileSize value="4096KB"/>
      <staticLogFileName value="true"/>
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date %-5p [%property{HostIP}]  : %message%newline"/>
        <!--<conversionPattern value="%d [%property{HostIP}] %-5p : %message%newline" />-->
        <!--<conversionPattern value="[%d] %-5p : %m%n" />-->
      </layout>
    </appender>
  </log4net>
  <appSettings>
    <add key="ConsumerKey" value="jacu3estanatHudR"/>
    <add key="ConsumerSecret" value="CrEmase8u55MeChU7reBrucRu3Ra3uSa"/>
    <add key="OAuthEndpoint" value="https://au.resdiary.com/OAuth/V10a"/>
    <add key="ServiceBaseUri" value="http://au.resdiary.com/WebServices/Epos/V1"/>
    <add key="SecondSecret" value="Of8sXTQ7QeYZs3Ee"/>
    <add key="DevelopmentRestaurantId" value="5133"/>
  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
  </startup>
</configuration>
